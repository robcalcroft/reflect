{
  "name": "reflect",
  "version": "0.0.0",
  "description": "A retrospective tool for teams",
  "main": "index.js",
  "repository": "git@github.com:robcalcroft/reflect.git",
  "author": "Rob Calcroft <rob@calcroft.com>",
  "license": "None",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.4.0",
    "apollo-server": "^2.5.0",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "pg": "^7.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-beautiful-dnd": "^11.0.3",
    "react-dom": "^16.8.6"
  },
  "engines": {
    "node": "10"
  },
  "scripts": {
    "format": "prettier --write \"**/*\"",
    "start": "run-p start:*",
