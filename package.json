{
  "name": "reflect",
  "version": "0.0.0",
  "description": "A retrospective tool for teams",
  "main": "index.js",
  "repository": "git@github.com:robcalcroft/reflect.git",
  "author": "Rob Calcroft <rob@calcroft.com>",
  "license": "None",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.4.0",
    "apollo-server": "^2.5.0",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "pg": "^7.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-beautiful-dnd": "^11.0.3",
    "react-dom": "^16.8.6"
  },
  "engines": {
    "node": "10"
  },
  "scripts": {
    "db:add-data": "node scripts/addData",
    "db:drop": "node scripts/dropDb",
    "db:init": "run-s db:drop db:setup db:add-data",
    "db:setup": "node scripts/setupDb",
    "start": "run-p start:*",
    "start:api": "nodemon -q --watch src/api src/api/index.js",
    "start:app": "parcel src/app/index.html --open",
    "build:app": "parcel build src/app/index.html",
    "lint:js": "eslint .",
    "lint:css": "prettier src/app/**/*.css --list-different",
    "test": "run-p lint:*"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.1"
  }
}
